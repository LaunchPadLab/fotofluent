{
  "manifest_version": 2,
  "name": "FotoFluent",
  "description": "A Chrome extension that helps you learn a new language!",
  "version": "1.0.0",
  "author": "Diana Camacho, Rachel Killackey",
  "chrome_url_overrides": {
    "newtab": "pages/tab.html"
  },
  "permissions": [
    "https://fotofluent-admin.herokuapp.com/*",
    "storage",
    "topSites",
    "tts"
  ],
  "web_accessible_resources": [
    "pages/*",
    "scripts/*"
  ],
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": true
  }
}
